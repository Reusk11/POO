package tarea;
import cuartosCasa.Baño;
import cuartosCasa.Cocina;
import cuartosCasa.Recamara;

public class Casa {
    private Baño baño;
    private Cocina cocina;
    private Recamara recamara;

    public Casa() {
    }

    public Casa(Baño baño, Cocina cocina, Recamara recamara) {
        this.baño = baño;
        this.cocina = cocina;
        this.recamara = recamara;
    }

    public Baño getBaño() {
        return baño;
    }

    public void setBaño(Baño baño) {
        this.baño = baño;
    }

    public Cocina getCocina() {
        return cocina;
    }

    public void setCocina(Cocina cocina) {
        this.cocina = cocina;
    }

    public Recamara getRecamara() {
        return recamara;
    }

    public void setRecamara(Recamara recamara) {
        this.recamara = recamara;
    }
    public void encenderLuz(){
        System.out.println("Encendiendo luces de casa");
    }

    @Override
    public String toString() {
        String data= "Baño:"+baño;
        data= data +"\nRecamara:"+recamara;
        data = data+"\nCocina:"+cocina;
        return data;
        
    }
    
    
    
}
